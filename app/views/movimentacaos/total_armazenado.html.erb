<table class="table table-striped table-responsive">
  <thead>
    <th>Deposito</th>
    <th>Produto</th>
    <th>Quantidade</th>
  </thead>
  <tbody>
  <%@movimentacoes.each do |movimentacao| %>
    <tr>
      <td><%=movimentacao[0][0] rescue nil %></td>
      <td><%=movimentacao[0][1] rescue nil%></td>
      <td><%=movimentacao[1] rescue nil%></td>
    </tr>
  <%end%>
  </tbody>
</table>
<div class="row">
  <div class="form-group">
    <%= link_to 'Produtos',produtos_path, method: :get, :class => 'btn btn-primary' %>
  </div>
  <div class="form-group col-sm-1">
    <%= link_to 'Armazenamento',armazenamentos_path, method: :get, :class => 'btn btn-success' %>
  </div>
  <div class="form-group col-sm-1">
    <%= link_to 'Movimentações',movimentacaos_path, method: :get, :class => 'btn btn-danger'%>
  </div>
  <div class="form-group col-sm-1">
    <%= link_to 'HOME',welcome_index_path, method: :get, :class => 'btn btn-dark'%>
  </div>
</div>