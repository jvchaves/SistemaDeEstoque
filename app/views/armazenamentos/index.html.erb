<p id="notice"><%= notice %></p>

<h1>Armazenamentos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @armazenamentos.each do |armazenamento| %>
      <tr>
        <td><%= armazenamento.nome %></td>
        <td><%= link_to 'Show', armazenamento %></td>
        <td><%= link_to 'Edit', edit_armazenamento_path(armazenamento) %></td>
        <td><%= link_to 'Destroy', armazenamento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Armazenamento', new_armazenamento_path %>
